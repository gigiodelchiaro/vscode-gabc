{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "GABC",
	"patterns": [
	  { "include": "#comments" },
	  { "include": "#headers" },
	  { "include": "#gabc-syllable-markup" },
	  { "include": "#gabc-syllable-controls" },
	  { "include": "#gabc-other" },
	  { "include": "#regions" }
	],
	"repository": {
	  "comments": {
		"patterns": [
		  {
			"name": "comment.line.percentage.gabc",
			"begin": "%",
			"end": "$"
		  }
		]
	  },
	  "headers": {
		"patterns": [
		  {
			"name": "entity.header.gabc",
			"match": "^([^:]+): *(.*);$",
			"captures": {
			  "1": { "name": "keyword.control.header.key.gabc" },
			  "2": { "name": "entity.other.attribute-name.header.value.gabc" }
			}
		  }
		]
	  },
	  "gabc-syllable-markup": {
		"patterns": [
		  {
			"contentName": "markup.bold.syllable.gabc",
			"begin": "<b>",
			"end": "</b>",
			"beginCaptures": { "0": { "name": "entity.name.type.bold.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.bold.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.deleted.colored.syllable.gabc",
			"begin": "<c>",
			"end": "</c>",
			"beginCaptures": { "0": { "name": "entity.name.type.colored.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.colored.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.italic.syllable.gabc",
			"begin": "<i>",
			"end": "</i>",
			"beginCaptures": { "0": { "name": "entity.name.type.italic.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.italic.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.other.smallcaps.syllable.gabc",
			"begin": "<sc>",
			"end": "</sc>",
			"beginCaptures": { "0": { "name": "entity.name.type.smallcaps.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.smallcaps.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.other.teletype.syllable.gabc",
			"begin": "<tt>",
			"end": "</tt>",
			"beginCaptures": { "0": { "name": "entity.name.type.teletype.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.teletype.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.underline.syllable.gabc",
			"begin": "<ul>",
			"end": "</ul>",
			"beginCaptures": { "0": { "name": "entity.name.type.underlined.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.underlined.syllable.close.gabc" } }
		  }
		]
	  },
	  "gabc-syllable-controls": {
		"patterns": [
		  {
			"contentName": "markup.other.clear.syllable.gabc",
			"begin": "<clear>",
			"end": "</clear>",
			"beginCaptures": { "0": { "name": "entity.name.type.clear.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.clear.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.italic.elision.syllable.gabc",
			"begin": "<e>",
			"end": "</e>",
			"beginCaptures": { "0": { "name": "entity.name.type.elision.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.elision.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.other.euouae.syllable.gabc",
			"begin": "<eu>",
			"end": "</eu>",
			"beginCaptures": { "0": { "name": "entity.name.type.euouae.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.euouae.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.other.no-line-break.syllable.gabc",
			"begin": "<nlba>",
			"end": "</nlba>",
			"beginCaptures": { "0": { "name": "entity.name.type.no-line-break.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.no-line-break.syllable.close.gabc" } }
		  },
		  {
			"name": "entity.name.type.protrusion.syllable.gabc",
			"match": "<pr(:.?[0-9])?/?>"
		  }
		]
	  },
	  "gabc-other": {
		"patterns": [
		  {
			"contentName": "markup.other.above-lines-text.syllable.gabc",
			"begin": "<alt>",
			"end": "</alt>",
			"beginCaptures": { "0": { "name": "entity.name.type.above-lines-text.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.above-lines-text.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.other.special.syllable.gabc",
			"begin": "<sp>",
			"end": "</sp>",
			"beginCaptures": { "0": { "name": "entity.name.type.special.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.special.syllable.close.gabc" } }
		  },
		  {
			"contentName": "markup.other.verbatim.syllable.gabc",
			"begin": "<v>",
			"end": "</v>",
			"beginCaptures": { "0": { "name": "entity.name.type.verbatim.syllable.open.gabc" } },
			"endCaptures": { "0": { "name": "entity.name.type.verbatim.syllable.close.gabc" } },
			"patterns": [
			  {
				"name": "entity.name.function.latex.verbatim.syllable.gabc",
				"match": "\\\\[a-zA-Z@]+"
			  }
			]
		  }
		]
	  },
	  "gabc-notation": {
		"patterns": [
		  {
			"name": "entity.name.tag.pitch.gabc",
			"match": "[a-np]"
		  },
		  {
			"name": "variable.parameter.note.shape.accidental.gabc",
			"match": "[a-np][xy#]"
		  },
		  {
			"name": "variable.parameter.note.shape.initio-debilis.gabc",
			"match": "-"
		  },
		  {
			"name": "variable.parameter.note.shape.oriscus.gabc",
			"match": "[Oo][01]?"
		  },
		  {
			"name": "variable.parameter.note.shape.pes-quadratum.gabc",
			"match": "q"
		  },
		  {
			"name": "variable.parameter.note.shape.quilisma.gabc",
			"match": "[Ww]"
		  },
		  {
			"name": "variable.parameter.note.shape.trivirga.gabc",
			"match": "vvv"
		  },
		  {
			"name": "variable.parameter.note.shape.bivirga.gabc",
			"match": "vv"
		  },
		  {
			"name": "variable.parameter.note.shape.virga.gabc",
			"match": "[Vv]"
		  },
		  {
			"name": "variable.parameter.note.shape.tristropha.gabc",
			"match": "sss"
		  },
		  {
			"name": "variable.parameter.note.shape.distropha.gabc",
			"match": "ss"
		  },
		  {
			"name": "variable.parameter.note.shape.stropha.gabc",
			"match": "s"
		  },
		  {
			"name": "variable.parameter.note.shape.linea.gabc",
			"match": "="
		  },
		  {
			"name": "variable.parameter.note.shape.liquescent.gabc",
			"match": "[~<>]"
		  },
		  {
			"name": "variable.parameter.note.shape.liquescent.gabc",
			"match": "[~<>]"
		  },
		  {
			"name": "variable.parameter.note.shape.cavum.gabc",
			"match": "r[0-8]?"
		  },
		  {
			"name": "variable.parameter.note.shape.surrounded.gabc",
			"match": "R"
		  },
		  {
			"name": "variable.parameter.clef.gabc",
			"match": "[cf]b?[1-5]"
		  },
		  {
			"name": "keyword.other.note.punctum.inclinatum.gabc",
			"match": "[A-NP][012]?"
		  },
		  {
			"name": "variable.parameter.symbol.episema.gabc",
			"match": "_[0-5]*"
		  },
		  {
			"name": "variable.parameter.symbol.ictus.gabc",
			"match": "['][01]?"
		  },
		  {
			"name": "variable.parameter.symbol.punctum-mora-vocis.gabc",
			"match": "\\."
		  },
		  {
			"name": "variable.parameter.separation.virgula.gabc",
			"match": "`0?"
		  },
		  {
			"name": "variable.parameter.separation.divisio.minimis.gabc",
			"match": "\\^0?"
		  },
		  {
			"name": "variable.parameter.separation.divisio.minima.gabc",
			"match": ",[0_]?"
		  },
		  {
			"name": "variable.parameter.separation.divisio.minor.gabc",
			"match": ";[1-6]?"
		  },
		  {
			"name": "variable.parameter.separation.divisio.finalis.gabc",
			"match": "::"
		  },
		  {
			"name": "variable.parameter.separation.divisio.maior.gabc",
			"match": ":\\??"
		  }
		]
	  },
	  "nabc": {
		"name": "region.nabc.gabc",
		"begin": "\\|",
		"end": "[\\|\\)]",
		"beginCaptures": { "0": { "name": "keyword.control.nabc.open.gabc" } },
		"endCaptures": { "0": { "name": "keyword.control.nabc.close.gabc" } }
	  },
	  "regions": {
		"patterns": [
		  {
			"name": "region.paren.gabc",
			"begin": "\\(",
			"end": "\\)",
			"beginCaptures": { "0": { "name": "markup.punctuation.paren.open.gabc" } },
			"endCaptures": { "0": { "name": "markup.punctuation.paren.close.gabc" } },
			"patterns": [
			  { "include": "#gabc-notation" },
			  { "include": "#nabc" }
			]
		  }
		]
	  }
	},
	"scopeName": "source.gabc"
  }