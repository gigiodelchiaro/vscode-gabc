{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "GABC",
	"patterns": [
		{ "include": "#comments" },
		{ "include": "#headers" },
		{ "include": "#gabc-syllable-markup" },
		{ "include": "#gabc-syllable-controls" },
		{ "include": "#gabc-syllable-other" },
		{ "include": "#gabc-syllable-translation" },
		{ "include": "#gabc-notes-region" }
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.line.percentage.gabc",
					"begin": "%",
					"end": "$"
				}
			]
		},
		"headers": {
			"patterns": [
				{
					"name": "entity.header.gabc",
					"match": "^([^:]+): *(.*);$",
					"captures": {
						"1": { "name": "keyword.control.header.key.gabc" },
						"2": { "name": "string.header.value.gabc" }
					}
				}
			]
		},
		"latex-verbatim": {
			"patterns": [
				{
					"name": "entity.name.function.latex.verbatim.gabc",
					"match": "\\\\[a-zA-Z@]+"
				}
			]
		},
		"gabc-syllable-markup": {
			"patterns": [
				{
					"contentName": "markup.bold.syllable.gabc",
					"begin": "(<)b(>)",
					"end": "(</)b(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.bold.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.bold.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.bold.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.bold.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.bold.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.bold.syllable.close.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" }
					]
				},
				{
					"begin": "(<)c(>)",
					"end": "(</)c(>)",
					"beginCaptures": {
						"0": { "name": "string.colored.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.colored.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.colored.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "invalid.colored.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.colored.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.colored.syllable.close.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" }
					]
				},
				{
					"contentName": "markup.italic.syllable.gabc",
					"begin": "(<)i(>)",
					"end": "(</)i(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.italic.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.italic.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.italic.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.italic.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.italic.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.italic.syllable.close.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" }
					]
				},
				{
					"begin": "(<)sc(>)",
					"end": "(</)sc(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.smallcaps.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.smallcaps.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.smallcaps.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.smallcaps.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.smallcaps.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.smallcaps.syllable.close.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" }
					]
				},
				{
					"begin": "(<)tt(>)",
					"end": "(</)tt(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.teletype.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.teletype.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.teletype.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.teletype.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.teletype.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.teletype.syllable.close.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" }
					]
				},
				{
					"contentName": "markup.underline.syllable.gabc",
					"begin": "(<)ul(>)",
					"end": "(</)ul(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.underlined.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.underlined.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.underlined.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.underlined.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.underlined.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.underlined.syllable.close.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" }
					]
				}
			]
		},
		"gabc-syllable-controls": {
			"patterns": [
				{
					"begin": "(<)clear(>)",
					"end": "(</)clear(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.clear.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.clear.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.clear.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.clear.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.clear.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.clear.syllable.close.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" },
						{ "include": "#gabc-notes-region" }
					]
				},
				{
					"contentName": "markup.italic.elision.syllable.gabc",
					"begin": "(<)e(>)",
					"end": "(</)e(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.elision.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.elision.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.elision.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.elision.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.elision.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.elision.syllable.open.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" }
					]
				},
				{
					"begin": "(<)eu(>)",
					"end": "(</)eu(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.euouae.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.euouae.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.euouae.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.euouae.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.euouae.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.euouae.syllable.close.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" },
						{ "include": "#gabc-notes-region" }
					]
				},
				{
					"begin": "(<)nlba(>)",
					"end": "(</)nlba(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.no-line-break.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.no-line-break.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.no-line-break.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.no-line-break.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.no-line-break.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.no-line-break.syllable.close.gabc" }
					},
					"patterns": [
						{ "include": "#gabc-syllable-markup" },
						{ "include": "#gabc-syllable-controls" },
						{ "include": "#gabc-syllable-other" },
						{ "include": "#gabc-syllable-translation" },
						{ "include": "#gabc-notes-region" }
					]
				},
				{
					"name": "entity.name.tag.protrusion.syllable.gabc",
					"match": "(<)pr(:.?[0-9])?(/?>)",
					"captures": {
						"1": { "name": "punctuation.definition.tag.begin.protrusion.syllable.gabc" },
						"3": { "name": "punctuation.definition.tag.end.protrusion.syllable.gabc" }
					}
				}
			]
		},
		"gabc-syllable-other": {
			"patterns": [
				{
					"contentName": "string.above-lines-text.syllable.gabc",
					"begin": "(<)alt(>)",
					"end": "(</)alt(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.above-lines-text.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.above-lines-text.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.above-lines-text.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.above-lines-text.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.above-lines-text.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.above-lines-text.syllable.close.gabc" }
					}
				},
				{
					"contentName": "string.special.syllable.gabc",
					"begin": "(<)sp(>)",
					"end": "(</)sp(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.special.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.special.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.special.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.special.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.special.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.special.syllable.close.gabc" }
					}
				},
				{
					"begin": "(<)v(>)",
					"end": "(</)v(>)",
					"beginCaptures": {
						"0": { "name": "entity.name.tag.verbatim.syllable.open.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.verbatim.syllable.open.gabc" },
						"2": { "name": "punctuation.definition.tag.end.verbatim.syllable.open.gabc" }
					},
					"endCaptures": {
						"0": { "name": "entity.name.tag.verbatim.syllable.close.gabc" },
						"1": { "name": "punctuation.definition.tag.begin.verbatim.syllable.close.gabc" },
						"2": { "name": "punctuation.definition.tag.end.verbatim.syllable.close.gabc" }
					},
					"patterns": [ { "include": "#latex-verbatim" } ]
				}
			]
		},
		"gabc-syllable-translation": {
			"patterns": [
				{
					"name": "string.translation.syllable.gabc",
					"begin": "\\[",
					"end": "\\]",
					"beginCaptures": { "0": { "name": "string.translation.syllable.open.gabc" } },
					"endCaptures": { "0": { "name": "string.translation.syllable.close.gabc" } }
				}
			]
		},
		"gabc-notes-region": {
			"patterns": [
				{
					"name": "region.notes.gabc",
					"begin": "\\(",
					"end": "\\)",
					"beginCaptures": { "0": { "name": "region.notes.open.gabc" } },
					"endCaptures": { "0": { "name": "region.notes.close.gabc" } },
					"patterns": [
						{ "include": "#gabc-notes" },
						{ "include": "#nabc-region" }
					]
				}
			]
		},
		"gabc-notes-fusible-region": {
			"patterns": [
				{
					"name": "region.notes.fusible.gabc",
					"begin": "@\\[",
					"end": "\\]",
					"beginCaptures": { "0": { "name": "keyword.control.notes.fusible.open.gabc" } },
					"endCaptures": { "0": { "name": "keyword.control.notes.fusible.close.gabc" } },
					"patterns": [ { "include": "#gabc-notes" } ]
				}
			]
		},
		"gabc-notes": {
			"patterns": [
				{ "include": "#gabc-notes-spacing" },
				{ "include": "#gabc-notes-additional-symbols" },
				{ "include": "#gabc-notes-separation-bars" },
				{ "include": "#gabc-notes-clefs" },
				{ "include": "#gabc-notes-line-breaks" },
				{ "include": "#gabc-notes-choral-signs" },
				{ "include": "#gabc-notes-braces" },
				{ "include": "#gabc-notes-stem-length" },
				{ "include": "#gabc-notes-custom-ledger-lines" },
				{ "include": "#gabc-notes-slurs" },
				{ "include": "#gabc-notes-horizontal-episema-tuning" },
				{ "include": "#gabc-notes-above-lines-text" },
				{ "include": "#gabc-notes-macros" },
				{
					"name": "variable.parameter.symbol.custos.gabc",
					"match": "[a-np]\\+"
				},
				{
					"name": "variable.parameter.note.accident.sharp.gabc",
					"match": "[a-np]#"
				},
				{
					"name": "variable.parameter.note.accident.flat.gabc",
					"match": "[a-np]x"
				},
				{
					"name": "variable.parameter.note.accident.natural.gabc",
					"match": "[a-np]y"
				},
				{
					"name": "constant.character.note.pitch.gabc",
					"match": "[a-np]"
				},
				{
					"name": "constant.character.note.pitch.punctum-inclinatum.gabc",
					"match": "[A-NP]([012]?)",
					"captures": { "1": { "name": "constant.numeric.note.pitch.punctum-inclinatum.leaning.gabc" } }
				},
				{
					"name": "variable.parameter.note.shape.oriscus.gabc",
					"match": "o([01]?)",
					"captures": { "1": { "name": "constant.numeric.note.shape.oriscus.variant.gabc" } }
				},
				{
					"name": "variable.parameter.note.shape.oriscus.scapus.gabc",
					"match": "O([01]?)",
					"captures": { "1": { "name": "constant.numeric.note.shape.oriscus.variant.gabc" } }
				},
				{
					"name": "variable.parameter.note.shape.pes.quadratum.gabc",
					"match": "q"
				},
				{
					"name": "variable.parameter.note.shape.quilisma.gabc",
					"match": "w"
				},
				{
					"name": "variable.parameter.note.shape.quilisma.quadratum.gabc",
					"match": "W"
				},
				{
					"name": "variable.parameter.note.shape.trivirga.gabc",
					"match": "vvv"
				},
				{
					"name": "variable.parameter.note.shape.bivirga.gabc",
					"match": "vv"
				},
				{
					"name": "variable.parameter.note.shape.virga.gabc",
					"match": "v"
				},
				{
					"name": "variable.parameter.note.shape.virga.reversa.gabc",
					"match": "V"
				},
				{
					"name": "variable.parameter.note.shape.tristropha.gabc",
					"match": "sss"
				},
				{
					"name": "variable.parameter.note.shape.distropha.gabc",
					"match": "ss"
				},
				{
					"name": "variable.parameter.note.shape.stropha.gabc",
					"match": "s"
				},
				{
					"name": "variable.parameter.note.shape.liquescent.deminutus.gabc",
					"match": "~"
				},
				{
					"name": "variable.parameter.note.shape.liquescent.augmented.gabc",
					"match": ">"
				},
				{
					"name": "variable.parameter.note.shape.liquescent.diminished.gabc",
					"match": "<"
				},
				{
					"name": "variable.parameter.note.shape.linea.gabc",
					"match": "="
				},
				{
					"name": "variable.parameter.note.shape.cavum.surrounded.gabc",
					"match": "r0"
				},
				{
					"name": "variable.parameter.note.shape.cavum.gabc",
					"match": "r"
				},
				{
					"name": "variable.parameter.note.shape.quadratum.surrounded.gabc",
					"match": "R"
				},
				{
					"name": "variable.parameter.note.shape.initio-debilis.gabc",
					"match": "-"
				},
				{
					"name": "keyword.control.note.fusion.gabc",
					"match": "@"
				}
			]
		},
		"gabc-notes-spacing": {
			"patterns": [
				{
					"contentName": "constant.numeric.spacing.factored.gabc",
					"begin": "/\\[",
					"end": "\\]",
					"beginCaptures": { "0": { "name": "variable.parameter.spacing.factored.open.gabc" } },
					"endCaptures": { "0": { "name": "variable.parameter.spacing.factored.close.gabc" } }
				},
				{
					"name": "variable.parameter.spacing.half.same-neume.gabc",
					"match": "/0"
				},
				{
					"name": "variable.parameter.spacing.small.same-neume.gabc",
					"match": "/!"
				},
				{
					"name": "variable.parameter.spacing.medium.gabc",
					"match": "//"
				},
				{
					"name": "variable.parameter.spacing.small.gabc",
					"match": "/"
				},
				{
					"name": "variable.parameter.spacing.non-breaking.gabc",
					"match": "! "
				},
				{
					"name": "variable.parameter.spacing.zero.gabc",
					"match": "!"
				}
			]
		},
		"gabc-notes-additional-symbols": {
			"patterns": [
				{
					"name": "variable.parameter.symbol.punctum-mora-vocis.gabc",
					"match": "\\."
				},
				{
					"name": "variable.parameter.symbol.episema.gabc",
					"match": "_[0-5]*"
				},
				{
					"name": "variable.parameter.symbol.ictus.gabc",
					"match": "'[01]?"
				},
				{
					"name": "variable.parameter.symbol.accent.above.gabc",
					"match": "r1"
				},
				{
					"name": "variable.parameter.symbol.accent.grave.above.gabc",
					"match": "r2"
				},
				{
					"name": "variable.parameter.symbol.circle.above.gabc",
					"match": "r3"
				},
				{
					"name": "variable.parameter.symbol.semicircle.lower.above.gabc",
					"match": "r4"
